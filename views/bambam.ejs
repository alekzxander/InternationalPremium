<head>
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h4 class="text-center">ajouter un voyage</h2>
                <form role="form">
                        <div class="form-group">
                                <input class="form-control input-sm" type="file" name="img" placeholder="Image Upload">
                            </div>
                    <div class="form-group">
                        <input class="form-control input-sm" type="text" name="name" placeholder="Titre">
                    </div>
                    <div class="form-group">
                        <input class="form-control input-sm" type="date" name="dateA"placeholder="Date aller">
                    </div>
                    <div class="form-group">
                        <input class="form-control input-sm" type="date" name="dateR" placeholder="Date retour">
                    </div>
                    <div class="form-group">
                        <input class="form-control input-sm" type="text" name="sejour" placeholder="Nombre de jours">
                    </div>
                    <div class="form-group">
                        <input class="form-control input-sm" type="text" name="preview" placeholder="Descriptif du voyage">
                    </div>
                    <div class="form-group">
                        <input class="form-control input-sm" type="text" name="text" placeholder="Résumé du voyage">
                    </div>
                    <div class="col-md-12">
                            <button type="submit" class="btn btn-warning btn-lg">confirmez</button>
                        </div>
                </form>
            </div>
        </div>