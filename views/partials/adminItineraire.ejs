<head>
        <link rel="stylesheet" href="/css/bootstrap.min.css">
        <link rel="stylesheet" href="/stylesheets/style.css">
    </head>

    <div class="container-fluid">
            <div class="row content">
            <div class="col-sm-3 sidenav">
                <h4 class="text-center">Admin</h4>
                <ul class="nav nav-pills nav-stacked">
                <li class="active"><a class="text-center" href="/dashbord/">Home</a></li>
                <li><a class="text-center" href="/dashbord/card">Card</a></li>
                <li><a class="text-center" href="/dashbord/dashItineraire">Itineraires</a></li>
                </ul><br>
            </div>

<div class="col-sm-9">
        <h4 class="text-center"><small>Dashbord</small></h4>
        <hr>
        <h4 class="text-center">Ajoutez un lieux de visite</h4>

        <div class="row">
                <% voyages.forEach((carte)=>{ %>

                    <div class="col-md-4">
                        <div class="carte">
                            <img src="/images/<%= carte.img %>" alt="">
                            <div class="ville">
                                <h1>
                                    <%= carte.name %>
                                </h1>
                            </div>
                            <div class="date">
                                <h3>Voyage de
                                    <%= carte.sejour %> !</h3>
                                <p>
                                    <%= carte.dateA %> au
                                        <%= carte.dateR %>
                                </p>
                            </div>
                            <div class="details">
                                <p>
                                    <%= carte.preview %>
                                </p>
                            </div>
                            <div class="row">
                                <a class="btn btn-danger btn-lg" href="/ajoutLieux/<%= carte._id %> ">Ajouter un lieux de visite</a>
                            </div>
                        </div>
                    </div>
                    <% })%>
            </div>
</div>